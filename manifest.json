{
   "action": {
      "default_icon": "pokeball64.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "css": [ "onload.css" ],
      "js": [ "js/jquery-3.1.1.min.js", "js/sha256.min.js", "onload.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "ProfPeel is a tool that extracts data from web pages and exports it as Excel or CSV files",
   "icons": {
      "128": "pokeball128.png",
      "16": "pokeball16.png",
      "32": "pokeball32.png",
      "64": "pokeball64.png"
   },
   "incognito": "split",
   "manifest_version": 3,
   "name": "ProfPeel",
   "permissions": [ "webRequest", "activeTab" ],
   "version": "99.0"
}

